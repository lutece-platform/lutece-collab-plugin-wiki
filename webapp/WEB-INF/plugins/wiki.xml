<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>wiki</name>
    <class>fr.paris.lutece.plugins.wiki.service.WikiPlugin</class>
    <version>3.0.2-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>wiki.plugin.description</description>
    <provider>wiki.plugin.provider</provider>
    <provider-url>http://your.web.site.com</provider-url>
    <icon-url>images/admin/skin/plugins/wiki/wiki.png</icon-url>
    <copyright/>
    <core-version-dependency>
        <min-core-version>5.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>

    <!-- Search Indexer Service -->
    <search-indexers>
        <search-indexer>
            <search-indexer-class>
                fr.paris.lutece.plugins.wiki.search.WikiIndexer
            </search-indexer-class>
        </search-indexer>
    </search-indexers>

    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheets>
        <css-stylesheets-scope>xpage</css-stylesheets-scope>
        <css-stylesheet>wiki/wiki.css</css-stylesheet>
        <css-stylesheet>wiki/toggle_switch.css</css-stylesheet>
        <css-stylesheet>wiki/prism/prism.css</css-stylesheet>
    </css-stylesheets>
    
    <!-- Specific plugin CSS stylesheet  -->
    <javascript-files>
        <javascript-files-scope>xpage</javascript-files-scope>  
        <javascript-file>wiki/prettify/run_prettify.js?skin=desert</javascript-file>
        <javascript-file>wiki/prism/prism.js</javascript-file>
    </javascript-files>

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>wiki</application-id>
            <application-class>fr.paris.lutece.plugins.wiki.web.WikiApp</application-class>
        </application>
    </applications>

</plug-in>